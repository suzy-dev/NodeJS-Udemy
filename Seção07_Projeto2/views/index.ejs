<%- include('partials/header.ejs') %>
<body>
<%- include('partials/navbar.ejs') %>
<div class="container">
    <hr>
    <h1 style="font-family: Arial Narrow, sans-serif;" >BLOG HYPERSTECH</h1>
    <hr>
        <table class="table table-striped table-dark">
            <thead>
                <tr>
                    <th>ID</th>
                    <th>Categoria</th>
                    <th>Título</th>
                    <th>Slug</th>
                    <th>Ações</th>
                </tr>
            </thead>
            <tbody>
                <% articles.forEach((articles) => { %>
                <tr>
                    <td><%= articles.id %></td>
                    <td><%= articles.category.title %></td>
                    <td><%= articles.title.toUpperCase() %></td>
                    <td><%= articles.slug.toLowerCase() %></td>
                    <td>Ações</td>
                </tr>
                <% }) %>
            </tbody>
        </table>
        
     
    

</div>

</body>
<%- include('partials/footer.ejs') %>



